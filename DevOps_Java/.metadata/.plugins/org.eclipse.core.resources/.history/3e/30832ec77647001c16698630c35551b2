package Exam02;

import java.util.ArrayList;
import java.util.List;

public class Exam02 {
	public static void main(String[] args) {
		List<Score> li = new ArrayList<Score>();
		
		Score s = new Score();
		
		s.setName("홍길동");
		s.setKor(100);
		s.setEng(100);
		s.setMath(100);
		
		li.add(s);
		
		Score s1 = li.get(0);
		
		System.out.println(s1.getName());
		System.out.println(li.get(0).getName());
	
	}
}
